      "joi": "^17.13.3",
      "nest-winston": "^1.10.2",
      "pg": "^8.16.0",
      "pg-hstore": "^2.3.4",
      "prom-client": "^15.1.3",
      "reflect-metadata": "^0.2.2",
      "resend": "^4.6.0",
      "rxjs": "^7.8.1",
      "sequelize": "^6.37.7",
      "sequelize-typescript": "^2.1.6",
