{
   "name": "weather-api",
   "version": "1.0.0",
   "description": "",
   "author": "",
   "private": false,
   "workspaces": [
      "microservices/*"
   ],
   "license": "MIT",
   "scripts": {
      "check": "npm run precommit && npm run prepush && npm run start",
      "prepare": "husky",
      "precommit": "npm audit && npm run format && npm run lint",
      "prepush": "npm run test",
      "start": "nest start",
      "start:prod": "node dist/src/main",
      "dev": "nest start --watch",
      "debug": "nest start --debug --watch",
      "build": "npm --workspaces --if-present run build",
      "lint": "eslint \"**/*.ts\" --fix",
      "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,html,css}\"",
      "test": "npm --workspaces --if-present run test",
      "test:arch": "npm --workspaces --if-present run test:arch",
      "test:unit": "npm --workspaces --if-present run test:unit",
      "test:e2e": "npm --workspaces --if-present run test:e2e",
      "test:ui": "npm --workspaces --if-present run test:ui",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
   },
